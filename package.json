{
  "name": "apollo-server-starter",
  "version": "0.0.0",
  "description": "Apollo api with react codegen",
  "main": "bindings/index.umd.js",
  "module": "bindings/index.es5.js",
  "typings": "bindings/lib/index.d.ts",
  "files": [
    "bindings"
  ],
  "scripts": {
    "test": "jest",
    "build": "npm run tslint && npm run build-ts",
    "build-bindings": "rollup -c rollup.config.ts",
    "build-ts": "tsc",
    "generate": "graphql-codegen",
    "build-ts-ok": "tsc && echo ok || echo not ok",
    "dist": "npm install && tsc -p tsconfig.build.json && npm run tslint",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "tslint-fix": "tslint -c tslint.json -p tsconfig.json --fix",
    "precommit": "echo \"Build must pass before committing\" && npm run build",
    "help": "npm run build && node ./dist/index.js help",
    "dev": "NODE_ENV=development concurrently --kill-others \"yarn build-ts -w && yarn copy\" \"nodemon dist/index.js\"",
    "start": "node ./dist/index.js",
    "killport": "kill $(lsof -t -i :12002)",
    "deploy": "dotenv -e .env.ecs ./ecs-deploy.sh"
  },
  "author": "Ryan P. Hansen",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/rphansen91/apollo-server-starter"
  },
